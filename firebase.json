{
  "hosting": {
    "site": "uks-grom-test-link",
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "/service-worker.js",
        "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
      }
    ],
    "rewrites": [
      {
        "source": "/**",
        "destination": "/**/"
      },
      {
        "source": "**",
        "destination": "/404.html"
      }
    ]
  }
}
